{
	"name": "vscode-events",
	"displayName": "VS Code Events",
	"description": "Execution of commands on events emitted by Visual Studio Code",
	"preview": true,
	"version": "0.1.0",
	"engines": {
		"vscode": "^1.33.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"contributes": {
		"configuration": {
			"type": "object",
			"title": "VS Code Events",
			"properties": {
				"events.configurations": {
					"type": "array",
					"default": [],
					"description": "Specifies commands to be run conditionally when an event is emitted."
				}
			}
		}
	},
	"main": "./extension.js",
	"scripts": {
		"postinstall": "node ./node_modules/vscode/bin/install"
	},
	"devDependencies": {
		"vscode": "^1.1.28",
		"eslint": "^5.13.0"
	}
}
